Relazioni:
	conto(KEY N#, amm NOTNULL, tettomax, scadenza, tipo, -haun- utente.ID NOT NULL, -haun- datacreazione NOT NULL, -riferitoa- conto_di_rif)

	spesa(KEY(conto.n#, idop), data,categoria,descrizione,valore NOT NULL)

